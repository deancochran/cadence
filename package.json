{
	"name": "sveltekit-starter-pack",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prisma-generate": "npx prisma generate",
		"prisma-format": "npx prisma format",
		"prisma-studio": "npx prisma studio",
		"stripe-webhook-listen":"stripe listen --forward-to localhost:5173/stripe/webhook"
	},
	"devDependencies": {
		"@playwright/test": "^1.39.0",
		"@skeletonlabs/skeleton": "^2.5.0",
		"@skeletonlabs/tw-plugin": "^0.2.4",
		"@sveltejs/adapter-auto": "^2.1.1",
		"@sveltejs/kit": "^1.27.6",
		"@types/node": "^20.9.0",
		"@types/nodemailer": "^6.4.14",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"autoprefixer": "^10.4.16",
		"esbuild": "^0.19.5",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.31",
		"postcss-load-config": "^4.0.1",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"prisma": "^5.6.0",
		"stripe": "^14.4.0",
		"svelte": "^4.2.3",
		"svelte-check": "^3.6.0",
		"svelte-stripe": "^0.0.25",
		"sveltekit-superforms": "^1.10.2",
		"tailwindcss": "^3.3.5",
		"ts-node": "^10.9.1",
		"tslib": "^2.6.2",
		"tsx": "^3.14.0",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"vitest": "^0.34.6",
		"zod": "^3.22.4"
	},
	"type": "module",
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
		"@lucia-auth/adapter-prisma": "^3.0.2",
		"@lucia-auth/oauth": "^3.5.0",
		"@lucia-auth/sveltekit": "^0.6.11",
		"@prisma/client": "5.5.2",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/stripe-v3": "^3.1.32",
		"lucia": "^2.7.4",
		"lucia-auth": "^1.8.0",
		"nodemailer": "^6.9.7",
		"svelte-email": "^0.0.4",
		"sveltekit-flash-message": "^2.2.2",
		"update": "^0.7.4",
		"upgrade": "^1.1.0"
	},
	"pnpm": {
		"overrides": {
			"braces@<2.3.1": ">=2.3.1",
			"yargs-parser@<=5.0.0": ">=5.0.1",
			"glob-parent@<5.1.2": ">=5.1.2",
			"set-value@>=3.0.0 <4.0.1": ">=4.0.1",
			"set-value@<2.0.1": ">=2.0.1",
			"undici@<5.26.2": ">=5.26.2"
		}
	}
}
